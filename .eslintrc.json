{
    "env": {
      "browser": true,
      "es2021": true
    },
    
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["import","react", "prettier"],
    "rules": {
      "prettier/prettier": ["error", { "endOfLine": "auto" }],
      "react/react-in-jsx-scope": "off",
      "import/no-unresolved": "error",
      "no-unused-vars": "warn"
    }
  }
  

  